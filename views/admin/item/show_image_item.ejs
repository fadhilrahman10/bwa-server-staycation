<div
  class="tab-pane fade show active"
  id="show_image"
  role="tabpanel"
  aria-labelledby="show-image"
>
  <div class="card shadow mb-4 mt-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">
        <a href="/admin/item" class="btn btn-primary"
          ><i class="fas fa-angle-double-left"></i> Back</a
        >
      </h6>
    </div>
    <div class="card-body">
      <div class="row">
        <% for( let i = 0; i < item.imageId.length; i++ ) { %>
        <div class="col-md-4">
          <img
            src="https://admin-bwa-mern.herokuapp.com/<%= item.imageId[i].imageUrl %>"
            alt="image"
            style="width: 100%; display: block"
          />
        </div>
        <% } %>
      </div>
    </div>
  </div>
</div>
